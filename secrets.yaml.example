# ESPHome Secrets Configuration
# Copy this file to secrets.yaml and fill in your actual values
# DO NOT commit secrets.yaml to version control

# Primary WiFi Network
wifi_ssid: "YourPrimaryWiFiSSID"
wifi_password: "YourPrimaryWiFiPassword"

# Secondary WiFi Network (optional)
wifi_ssid2: "YourSecondaryWiFiSSID"
wifi_password2: "YourSecondaryWiFiPassword"

# Tertiary WiFi Network (optional)
wifi_ssid3: "YourTertiaryWiFiSSID"
wifi_password3: "YourTertiaryWiFiPassword"

# API Password for ESPHome Dashboard/Home Assistant
api_password: "YourAPIPassword"

# OTA Password (optional, for secure over-the-air updates)
ota_password: "YourOTAPassword"
